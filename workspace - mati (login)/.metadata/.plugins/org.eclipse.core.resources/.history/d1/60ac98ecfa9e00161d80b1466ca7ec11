package dao;

import java.util.List;

public abstract class DAO<T>{
	public abstract void insertar(T obj) throws DAOExeption;;
    public abstract void borrar(T obj)throws DAOExeption;
    public abstract void modificar(T obj) throws  DAOExeption;
    public abstract long buscar(T obj) throws DAOExeption;
    public abstract List<T> listarTodos() throws DAOExeption;
}
